{
    "name": "@cmmv/ui",
    "version": "0.0.3",
    "description": "",
    "license": "MIT",
    "author": "Andre Ferreira",
    "private": false,
    "scripts": {
        "clean": "rimraf dist",
        "clean:server": "./clean_dist.sh",
        "dev": "vite dev",
        "build": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types && vite build",
        "preview": "vite preview",
        "type-check": "vue-tsc --noEmit",
        "start": "node dist/client.js",
        "test": "vitest",
        "release": "pnpm run build && pnpm run clean:server && node scripts/release.js",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "generate": "node -r @swc-node/register ./docs/generate.ts",
        "prepare": "husky"
    },
    "files": [
        "dist",
        "src"
    ],
    "keywords": [
        "cmmv",
        "ui"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andrehrferreira/cmmv-ui.git"
    },
    "bugs": {
        "url": "https://github.com/andrehrferreira/cmmv-ui/issues"
    },
    "homepage": "https://github.com/andrehrferreira/cmmv-ui#readme",
    "type": "module",
    "main": "dist/cmmv-ui.cjs.js",
    "module": "dist/cmmv-ui.es.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/cmmv-ui.es.js",
            "require": "./dist/cmmv-ui.cjs.js"
        }
    },
    "lint-staged": {
        "**/*.{ts,json}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/node": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@cmmv/plugin-vite": "^0.0.2",
        "@swc-node/core": "^1.13.3",
        "@swc-node/register": "^1.10.9",
        "@swc/core": "^1.9.3",
        "@types/node": "^22.10.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vitejs/plugin-vue-jsx": "^4.1.1",
        "@vue/runtime-core": "^3.5.13",
        "babel-core": "^6.26.3",
        "babel-loader": "^9.2.1",
        "browserslist": "^4.24.2",
        "chalk": "^5.3.0",
        "concurrently": "^9.1.0",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0",
        "css-loader": "^7.1.2",
        "enquirer": "^2.4.1",
        "execa": "^9.5.1",
        "html-loader": "^5.1.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.10",
        "nodemon": "^3.1.7",
        "prettier": "^3.4.1",
        "release-it": "^17.10.0",
        "rimraf": "^6.0.1",
        "semver": "^7.6.3",
        "tailwindcss": "^3.4.15",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "unplugin-vue-components": "^0.27.5",
        "vite": "^5.4.11",
        "vite-plugin-dts": "^4.3.0",
        "vite-tsconfig-paths": "^5.1.3",
        "vue": "^3.5.13",
        "vue-loader": "^17.4.2",
        "vue-tsc": "^2.1.10"
    },
    "dependencies": {
        "@cmmv/core": "^0.6.0",
        "@cmmv/http": "^0.6.0",
        "@cmmv/repository": "^0.6.0",
        "@cmmv/view": "^0.6.0",
        "@heroicons/vue": "^2.2.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.5",
        "fast-glob": "^3.3.2",
        "fast-json-stringify": "^6.0.0",
        "glob": "^11.0.0",
        "highlight.js": "^11.10.0",
        "json5": "^1.0.2",
        "lodash": "^4.17.21",
        "markdown-it": "^14.1.0",
        "moment": "^2.30.1",
        "path-to-regexp": "^0.1.11",
        "postcss-nesting": "^13.0.1",
        "qs": "^6.13.1",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.20",
        "vue-highlightjs": "^1.3.3",
        "vue-i18n": "^10.0.4",
        "vue-router": "^4.5.0"
    }
}
